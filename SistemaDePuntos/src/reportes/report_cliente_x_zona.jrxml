	<queryString>
		<![CDATA[SELECT CONCAT(p.pnombre, ' ', p.snombre, ' ', p.papellido, ' ', p.sapellido) Cliente, CONCAT( p.telefono1,' ', p.telefono2, ' ', p.telefono3) Telefono,z.zona, c.puntos_actuales FROM Cliente c
